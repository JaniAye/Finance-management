<%@ page contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" %>
<!DOCTYPE html>
<html>
<head>
  <title>Finance Management System - Login</title>
  <link rel="stylesheet" href="css/loginForm.css">
</head>
<body>
  <section>
    <div class="main-form-container">
      <div id="form_section" class="form-container">
        <div class="login-form form-wraper ">
          <div>
            <div class="form-title">
              <h2>Login</h2>
            </div>
            <div class="input-group">
              <div class="box">
                <span>
                  <input placeholder="Email" class="myInput" type="text" />
                </span>
              </div>
            </div>
            <div class="input-group">
              <div class="box">
                <span>
                  <input placeholder="Password" class="myInput" type="text" />
                </span>
              </div>
            </div>
            <div class="forget-password">
              <a href="">FORGOT PASSWORD</a>
            </div>
            <div class="action-button">
              <button>Login</button>
            </div>
            <div class="action-button">
              <button>Register</button>
            </div>
          </div>
        </div>

      </div>

    </div>
  </section>
</body>
</html>